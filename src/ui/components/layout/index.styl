 .layout_container{
  width: 100%;
  height: 100%;
  position: relative;
  overflow-x: hidden;
}
.layout {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  overflow-y: hidden;
  
  display: -ms-flexbox;
  -ms-flex-direction: column;

  display:flex;
  display:-webkit-box;
  -webkit-flex-direction:column;
  -webkit-box-orient:vertical;


}

.layout_drawer{
  position: absolute;
  top: 0;
  left: 0;
  transform: translateX(-100%);
  width: 80%;
  height: 100%;
  background-color: black;
  box-sizing: border-box;
  overflow-y: auto;
  overflow-x: hidden;
  z-index: 9999;
}
.layout_drawer_menu{
  position: absolute;
  background-color: transparent;
  z-index: 9999;
  top: 7.5px;
  left: 15px;
  width: 30px;
  height: 30px;
}

.layout_floatAboveLayout{
  position: absolute !important;
  z-index: 9999 !important;
}
