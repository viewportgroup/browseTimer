.header {
  position: relative;
  width: 100%;
  height: 44px;
  box-sizing: border-box;
  border-bottom: 1px solid #ddd;
  background-color: transparent;
  background-image: none;
}
.header_title {
  width: 100%;
  height: 100%;
  text-align: center
}

.header_backButtonArea {
  position: absolute;
  left: 0;
  top: 5px;
  height: 34px;
  line-height: 34px;
  width: 35px;
  z-index: 1
}
.header_backButton {
  background-image: url('data:image/png;base64,');
  background-size: 12px 22px;
  background-repeat: no-repeat;
  width: 12px;
  height: 22px;
  position: absolute;
  top: 6px;
  left: 15px
}
